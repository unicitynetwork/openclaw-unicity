{
  "id": "openclaw-unicity",
  "channels": ["unicity"],
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "network": {
        "type": "string",
        "enum": ["testnet", "mainnet", "dev"],
        "default": "testnet"
      },
      "nametag": {
        "type": "string"
      },
      "owner": {
        "type": "string",
        "description": "Nametag or public key of the trusted human owner. Only the owner can give commands to the agent."
      },
      "additionalRelays": {
        "type": "array",
        "items": { "type": "string" }
      }
    }
  }
}
